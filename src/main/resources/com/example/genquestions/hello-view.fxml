<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.VBox?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.control.ProgressBar?>
<?import javafx.scene.layout.HBox?>
<VBox alignment="CENTER" spacing="20.0" xmlns:fx="http://javafx.com/fxml"
      fx:controller="com.example.genquestions.HelloController">
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0"/>
    </padding>

    <VBox>
        <Label text="Input"/>
        <HBox>
            <TextField fx:id="textFieldInput" text="C:\Users\moska\Downloads\portugal.xlsx"/>
            <Button fx:id="btnFile" text="File" onAction="#onChoiceFileButtonClick" />
        </HBox>
    </VBox>
    <VBox>
        <Label text="Output"/>
        <TextField fx:id="textFieldOutput" text="test"/>
    </VBox>
    <ProgressBar fx:id="progressBar" visible="false" />
    <Button fx:id="btnGenerate" text="Generate" onAction="#onGenerateButtonClick"/>
</VBox>
